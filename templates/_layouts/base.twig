{% spaceless %}
<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml" lang="en-US" class="antialiased">
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta charset="utf-8" />
    <title>
      {% if entry.title is defined %}{{ entry.title }} - {% endif %}{{ siteName }}
    </title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"  />
    
    {% cache globally for 3 weeks %}
        {% if trackingCodes.headerCodes %}
          {{ trackingCodes.headerCodes|raw }}
        {% endif %}
    {% endcache %}

      {% do view.registerCssFile("/dist/styles.css") %}
      {% do view.registerCssFile("/fontawesome/css/all.css") %}
      {% do view.registerJsFile('/fontawesome/js/all.js') %}

      <link rel="stylesheet" href="https://use.typekit.net/iwi6una.css">

      <script
              src="https://code.jquery.com/jquery-3.4.1.min.js"
              integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo="
              crossorigin="anonymous"></script>
      <script src="https://unpkg.com/swiper/js/swiper.min.js"></script>

      <link rel="stylesheet" href="https://unpkg.com/swiper/css/swiper.min.css">

      <script src="https://kit.fontawesome.com/636c179e8c.js" crossorigin="anonymous"></script>

    {% block head %}{% endblock %}

 </head>
 <body class="{% if bodyClass is defined %}{{ bodyClass }}{% endif %} font-body">

 {% if alerts.alertText %}
     <div class="w-full text-center bg-red text-white text-lg p-4">
         <p class="mb-0 leading-none">{{ alerts.alertText }}</p>
     </div>
 {% endif %}
  
  {% cache globally for 3 weeks %}
    {% if trackingCodes.bodyCodes %}
      {{ trackingCodes.bodyCodes|raw }}
    {% endif %}
  {% endcache %}
   {% block body %}{% endblock %}  

   {% do view.registerJsFile("/dist/main.js") %}

   {% block foot %}{% endblock %}

  
  {% cache globally for 3 weeks %}
    {% if trackingCodes.footerCodes %}
      {{ trackingCodes.footerCodes }}
    {% endif %}
  {% endcache %}

 <script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>
    <script type="text/javascript">
      function googleTranslateElementInit() {
          new google.translate.TranslateElement({pageLanguage: 'en', includedLanguages: 'es,zh-CN,hr,tl,de,el,hi,it,mk,pl,sr,tr,vi'}, 'google_translate_element');
      }
    </script>

 <style>
          .goog-logo-link {
              display:none !important;
          }

          .goog-te-gadget{
              color: transparent !important;
          }
      </style>
 </body> 
</html>
{% endspaceless %}