{
  "require": {
    "aelvan/imager": "v2.3.1",
    "craftcms/cms": "3.4.1",
    "craftcms/redactor": "2.5.0",
    "dgrigg/craft-migration-assistant": "3.1.6",
    "ether/seo": "3.6.3",
    "luwes/craft3-codemirror": "1.0.2",
    "spicyweb/craft-neo": "2.6.0",
    "topshelfcraft/wordsmith": "3.1.1",
    "verbb/super-table": "2.4.0",
    "vlucas/phpdotenv": "^3.4.0",
    "youandmedigital/breadcrumb": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
