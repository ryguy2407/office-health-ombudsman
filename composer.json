{
  "require": {
    "aelvan/imager": "v2.4.0",
    "craftcms/cms": "3.4.9",
    "craftcms/feed-me": "4.2.0.1",
    "craftcms/redactor": "2.6.0.1",
    "dgrigg/craft-migration-assistant": "3.1.6",
    "ether/seo": "3.6.3",
    "luwes/craft3-codemirror": "1.0.2",
    "nystudio107/craft-seomatic": "3.2.47",
    "putyourlightson/craft-blitz": "3.5.0",
    "solspace/craft-freeform": "3.6.5",
    "spicyweb/craft-neo": "2.6.3",
    "topshelfcraft/wordsmith": "3.1.2",
    "verbb/field-manager": "2.1.1",
    "verbb/navigation": "1.3.6",
    "verbb/super-table": "2.4.6",
    "vlucas/phpdotenv": "^3.4.0",
    "youandmedigital/breadcrumb": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
